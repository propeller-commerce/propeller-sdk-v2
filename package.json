{
  "name": "propeller-sdk-v2",
  "version": "0.1.21",
  "description": "TypeScript GraphQL client for Propeller eCommerce V2 Platform - works with React, Vue, Angular and all JavaScript frameworks",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:graphql": "node scripts/build-graphql-bundle.js",
    "build:tsc": "tsc",
    "build": "npm run build:graphql && npm run build:tsc",
    "build:docs": "node scripts/build-docs.js",
    "clean": "rimraf dist src/generated",
    "clean:docs": "rimraf documentation",
    "prebuild": "npm run clean",
    "typecheck": "npm run build:graphql && tsc --noEmit",
    "prepare": "npm run build",
    "prepack": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "validate": "npm run typecheck && npm run build",
    "prepublishOnly": "npm run validate",
    "docs:build": "typedoc",
    "docs:serve": "http-server documentation -p 8080 -o",
    "docs:watch": "typedoc --watch",
    "docs:clean": "rimraf documentation",
    "docs:full": "npm run build:docs"
  },
  "keywords": [
    "graphql",
    "typescript",
    "propeller",
    "ecommerce",
    "client",
    "api",
    "sdk",
    "v2",
    "react",
    "vue",
    "angular",
    "frontend",
    "browser",
    "javascript",
    "framework-agnostic"
  ],
  "author": "Propeller Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/propeller-commerce/propeller-sdk-v2.git"
  },
  "files": [
    "dist/**/*",
    "documentation/**/*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "dotenv": "^16.3.1",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.28.11",
    "typedoc-neo-theme": "^1.1.1",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "graphql": "^16.11.0"
  }
}
